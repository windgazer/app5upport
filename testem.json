{
    "launchers": {
        "JasmineNode": {
            "command": "jasmine-node test/node"
        },
        "CSSLint": {
            "command": "csslint css"
        }
    },
    "launch_in_dev": [
        "PhantomJS",
        "CSSLint"
    ],
    "src_files": [
        "*.js",
        "test/specs/**/*.js"
    ],
    "watch_files": [
        "*.js",
        "**/*.css"
    ]
}